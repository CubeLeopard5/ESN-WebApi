{
  "ConnectionStrings": {
    "DefaultConnection": "dotnet_secrets"
  },
  "Jwt": {
    "Key": "dotnet_secrets",
    "Issuer": "YourApp",
    "Audience": "YourAppUsers",
    "ExpireMinutes": 30
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "https://localhost:3000"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
